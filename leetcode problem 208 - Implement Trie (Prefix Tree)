#leetcode problem 208 - Implement Trie (Prefix Tree)

class Trie(object):

    def __init__(self):
        self.root={}

    def insert(self, word):
        c=self.root
        for i in word:
            if i not in c:
                c[i]={}
            c=c[i]
        c['end']=''

    def search(self, word):
        c=self.root
        for i in word:
            if i not in c:
                return False
            c=c[i]
        return 'end' in c
        
    def startsWith(self, prefix):
                return False
            c=c[i]
        return True
